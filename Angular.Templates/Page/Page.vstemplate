  <TemplateContent>
    <References />
    <ProjectItem ReplaceParameters="true" TargetFileName="$folderName$\\$tsFileName$">page-component.ts</ProjectItem>
    <ProjectItem ReplaceParameters="true" TargetFileName="$folderName$\\$htmlFileName$">page-component.html</ProjectItem>
    <ProjectItem ReplaceParameters="true" TargetFileName="$folderName$\\$cssFileName$">page-component.scss</ProjectItem>
  </TemplateContent>
  <WizardExtension>
    <Assembly>Angular.Wizards, Version=1.0.0.0, Culture=neutral, PublicKeyToken=647581456131c645</Assembly>
    <FullClassName>Angular.Wizards.Page.PageWizard</FullClassName>
  </WizardExtension>
