  <TemplateData>
    <Name>Angular Page Component</Name>
    <Description>Creates the set of files for a page component.</Description>
    <Icon>Angular.ico</Icon>
    <TemplateID>6c916c03-81f8-4f57-8137-81e2ba0d339a</TemplateID>
    <ProjectType>CSharp</ProjectType>
    <TemplateGroupID>AspNetCore</TemplateGroupID>
    <RequiredFrameworkVersion>2.0</RequiredFrameworkVersion>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
    <DefaultName>page1.page-component.ts</DefaultName>
